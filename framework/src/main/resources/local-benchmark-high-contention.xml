<local-bench-config>

   <benchmark>
      <Warmup operationCount="5000000"/>
      <!-- HIGH contention.  100 threads working on 25 keys. -->
      <StressTest numRequests="25000000" opsCountStatusLog="100000" numThreads="100" sysMonitorEnabled="true" numEntries="25" sharedKeys="true"/>
   </benchmark>

   <products>
      <infinispan53>
         <config name="local-config.xml" cache="x"/>
      </infinispan53>
   </products>

   <reports>
      <report name="all_included" includeAll="true"/>
   </reports>
</local-bench-config>
      